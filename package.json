  "main": "node ./build/api/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "npm run build:app && tsc --project ./tsconfig.api.json --listEmittedFiles",
    "build:app": "rm -fr ./build && npx remix vite:build && mv build/server/index.js build/server/index.mjs",
    "build:serve": "npm run build && npm start",
    "build:app:serve": "npm run build:app && npx remix dev",
    "start": "node build/api/index.js",
    "start:dev": "NODE_OPTIONS='--no-warnings' nodemon --watch 'src/**/*' --ignore build -e ts,tsx --exec 'npm run build:app && ts-node --project tsconfig.api.json ./src/api/index.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
